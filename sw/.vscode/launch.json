    "version": "0.2.0",
    "configurations": [
        {
            "name": "Client",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/client/src/main.py",
            "console": "integratedTerminal",
            "args": ["-c", "./client/cfg/debug_cfg.json", "-l", "./client/cfg/debug_loggers_cfg.json"]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server/src/main.go",
            "showLog": true
        }
    ]
}